#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define LIBCOIN_SEEDER_VERSION       $libcoin_seeder.version.project_number$ULL
#define LIBCOIN_SEEDER_VERSION_STR   "$libcoin_seeder.version.project$"
#define LIBCOIN_SEEDER_VERSION_ID    "$libcoin_seeder.version.project_id$"

#define LIBCOIN_SEEDER_VERSION_MAJOR $libcoin_seeder.version.major$
#define LIBCOIN_SEEDER_VERSION_MINOR $libcoin_seeder.version.minor$
#define LIBCOIN_SEEDER_VERSION_PATCH $libcoin_seeder.version.patch$

#define LIBCOIN_SEEDER_PRE_RELEASE   $libcoin_seeder.version.pre_release$

#define LIBCOIN_SEEDER_SNAPSHOT_SN   $libcoin_seeder.version.snapshot_sn$ULL
#define LIBCOIN_SEEDER_SNAPSHOT_ID   "$libcoin_seeder.version.snapshot_id$"
